import java.time.Instant;
import java.util.ArrayList;
import java.util.List;


public class StockMarket {


	public static double GBCE(List<Double> prices){
		double buff=1f;
		for (Double f:prices)
			buff*=f;

		return nRoot(buff,prices.size(),0);
	}

	public static double nRoot( double nr,int root, double p) {
		if(nr < 0) {
			System.err.println("Number is < 0");// we handle only real positive numbers
			return -1.0;
		} else if(nr == 0) {
			return 0;
		}
		double x_prev = nr;
		double x = nr / root;  // starting "guessed" value...
		while(Math.abs(x - x_prev) > p) {
			x_prev = x;
			x = ((root - 1.0) * x + nr / Math.pow(x, root - 1.0)) / root;
		}
		return x;
	}


}
